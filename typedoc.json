{
  "entryPoints": [
    "src/mod.ts",
    "src/compiler/mod.ts",
    "src/deserializer/mod.ts",
    "src/serializer/mod.ts"
  ],
  "excludePrivate": true,
  "name": "Charged URI",
  "out": "./target/typedoc",
  "compilerOptions": {
    "module": 7 /* ES2022 */,
    "skipLibCheck": true
  },
  "externalSymbolLinkMappings": {
    // {@link typescript!Partial} will use this link as well as
    // type Foo = Partial<Bar>
    "esgen": {
      "EsMemberVisibility": "https://ucnot.github.io/esgen/enums/EsMemberVisibility.html"
    }
  },
  "sort": ["static-first", "visibility", "enum-value-ascending", "alphabetical", "kind"]
}
