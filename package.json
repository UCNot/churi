{
  "name": "@hatsy/churi",
  "version": "0.3.0",
  "description": "Charged URI",
  "keywords": [
    "charged-uri",
    "matrix-uri",
    "matrix-url",
    "rfc3986",
    "uri",
    "uri-charge",
    "url"
  ],
  "homepage": "https://github.com/hatsyjs/churi",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:hatsyjs/churi.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hatsyjs/churi/issues"
  },
  "type": "module",
  "types": "./dist/churi.d.ts",
  "exports": {
    ".": {
      "types": "./dist/churi.d.ts",
      "default": "./dist/churi.js"
    },
    "./compiler": {
      "types": "./dist/churi.compiler.d.ts",
      "default": "./dist/churi.compiler.js"
    },
    "./deserializer": {
      "types": "./dist/churi.deserializer.d.ts",
      "default": "./dist/churi.deserializer.js"
    },
    "./serializer": {
      "types": "./dist/churi.serializer.d.ts",
      "default": "./dist/churi.serializer.js"
    }
  },
  "dependencies": {
    "@proc7ts/primitives": "^4.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@proc7ts/async": "^1.0.1",
    "@proc7ts/supply": "^2.0.0",
    "@run-z/eslint-config": "^3.3.1",
    "@run-z/prettier-config": "^2.0.0",
    "@run-z/project-config": "^0.15.1",
    "@swc/core": "^1.3.41",
    "@swc/jest": "^0.2.24",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-plugin-jest": "^27.2.1",
    "expect": "^29.5.0",
    "gh-pages": "^5.0.0",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "jest-mock": "^29.5.0",
    "prettier": "^2.8.5",
    "prettier-eslint-cli": "^7.1.0",
    "rollup": "^3.20.0",
    "run-z": "^1.11.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typedoc": "^0.23.28",
    "typescript": "~4.9.5"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z build:gen-sync --then build-z",
    "build:gen-sync": "run-z +z --then node ./build/gen-sync.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then clean-z",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "format": "run-z +z --then prettier-eslint --write --include-dot-files \"src/**/*.*\" \"*.{js,cjs,json,md}\"",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z env:NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" --then test-z",
    "z": "run-z +cmd:build-z,+cmd:typedoc,+cmd:eslint,+cmd:test-z"
  }
}
